  "repository": {
    "type": "git",
    "url": "https://github.com/AugustHome/server-health.git"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "git-repo-info": "^2.1.1",
    "lodash": "^4.17.21",
    "restify-errors": "^8.0.2"
  },
  "devDependencies": {
    "@hapi/hapi": "^21.4.0",
    "chai": "^5.2.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.6",
    "eslint-plugin-security": "^3.0.1",
    "express": "^4.21.2",
    "fastify": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "mocha": "^11.1.0",
    "mocha-multi": "^1.1.7",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "restify": "^11.1.0",
    "sinon": "^19.0.2"
