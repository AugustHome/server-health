{
  "name": "server-health",
  "version": "4.0.1",
  "description": "Library to provide server health information via a HTTP server endpoint for express, restify, hapi, and vanilla http",
  "keywords": [
    "server",
    "node",
    "http",
    "status",
    "uptime",
    "rest",
    "json",
    "ping",
    "health",
    "restify",
    "express",
    "hapi"
  ],
  "main": "index.js",
  "scripts": {
    "bootstrap": "scripts/bootstrap.sh",
    "setup": "scripts/setup.sh",
    "update": "scripts/update.sh",
    "test": "scripts/test.sh",
    "test:coverage": "scripts/test.sh -c",
    "lint": "./node_modules/.bin/eslint --color --ext .js lib test example",
    "lint:test": "npm run lint && npm run test",
    "example": "node example/server.js"
  },
  "author": {
    "name": "Ingo Renner",
    "email": "ingo@august.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AugustHome/server-health.git"
  },
  "engines": {
    "node": ">=6.11.1"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "bluebird": "^3.5.1",
    "git-repo-info": "^2.0.0",
    "lodash": "^4.17.20",
    "restify-errors": "^8.0.2"
  },
  "devDependencies": {
    "@hapi/hapi": "^20.0.0",
    "chai": "^4.1.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-security": "^1.4.0",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "mocha": "^8.1.3",
    "mocha-multi": "^1.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "restify": "^8.5.1",
    "sinon": "^9.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "scripts/pre-commit"
    }
  }
